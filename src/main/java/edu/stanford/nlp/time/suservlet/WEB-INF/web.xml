<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.3.dtd">

<web-app>

    <display-name>Stanford SUTime Servlet</display-name>
    <description>
        This is a simple JSP-based interface to the SUTime annotator.
    </description>

    <servlet>
        <servlet-name>SUTimeServlet</servlet-name>
        <servlet-class>edu.stanford.nlp.time.suservlet.SUTimeServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SUTimeServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>*.css</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>*.js</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>/favicon.ico</url-pattern>
    </servlet-mapping>
</web-app>
